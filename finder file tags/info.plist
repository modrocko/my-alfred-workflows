				<string>import os
import json
import subprocess

workflow_dir = os.environ["alfred_workflow_data"]
files_path = os.path.join(workflow_dir, "files.json")
default_tags_path = os.path.join(workflow_dir, "default-tags.json")
title = os.environ["alfred_workflow_name"]

# If already setup, skip
if os.path.exists(workflow_dir) and os.path.exists(files_path) and os.path.exists(default_tags_path):
    subprocess.run([
        "osascript", "-e",
        f'display notification "Setup skipped — files already exist." with title "{title}"'
    ])
    exit(0)

# Create directory
os.makedirs(workflow_dir, exist_ok=True)

# Create files.json
if not os.path.exists(files_path):
    with open(files_path, 'w') as f:
        json.dump([], f, indent=2)

# Create default-tags.json
if not os.path.exists(default_tags_path):
    default_tags_list = ["@project", "@reference", "@later"]
    with open(default_tags_path, 'w') as f:
        json.dump(default_tags_list, f, indent=2)

# Notify success
subprocess.run([
    "osascript", "-e",
    f'display notification "Setup complete — JSON files created." with title "{title}"'
])</string>
