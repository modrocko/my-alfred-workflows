import os
import json

data_path = os.getenv("alfred_workflow_data")
md_file = os.path.join(data_path, "tasks.md")
json_file = os.path.join(data_path, "tasks.json")

if not os.path.exists(md_file):
    os.system(f"""osascript -e 'display notification "No tasks.md file found" with title "To-Do Task Tags"'""")
    exit()

with open(md_file, "r") as f:
    lines = [line.strip() for line in f.readlines()]

entries = []
current_tag = None
current_tasks = []

for line in lines:
    if not line or line == "# Tasks":
        continue
    elif not line.startswith("-"):
        # Save previous tag if it exists
        if current_tag is not None:
            entries.append({"tag": current_tag, "tasks": current_tasks})
        current_tag = line
        current_tasks = []
    else:
        task = line[2:].strip()  # remove '- '
        current_tasks.append(task)

# Add final group
if current_tag is not None:
    entries.append({"tag": current_tag, "tasks": current_tasks})

with open(json_file, "w") as f:
    json.dump(entries, f, indent=2)

os.system(f"""osascript -e 'display notification "Imported from tasks.md" with title "To-Do Task Tags"'""")
